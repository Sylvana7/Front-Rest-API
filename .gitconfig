[alias]
    acp = !sh -c 'git add . && git commit -m "$1" && git push origin $(git symbolic-ref --short HEAD)'
    ac = !sh -c 'git add . && git commit -m "$1"'
    cacp = !sh -c 'git checkout -b "$1" && git add . && git commit -m "$1" && git push origin $(git symbolic-ref --short HEAD)'
    c = !sh -c 'git checkout -b "$1"'

